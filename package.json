{
  "name": "maginet-deploy",
  "version": "1.0.0",
  "description": "Electron application to deploy Maginet on Steam",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "npx electron-packager . --ignore=\"^/maginet$\" --ignore=\"^/node_modules$\" --overwrite",
    "build-mac": "npx electron-packager src --ignore=\"^/maginet$\" --ignore=\"^/node_modules$\" --overwrite --out=\"maginet-mac\" --arch=x64",
    "build-mac-demo": "npx electron-packager src --ignore=\"^/maginet$\" --ignore=\"^/node_modules$\" --overwrite --out=\"maginet-mac-demo\" --arch=x64"
  },
  "repository": "https://github.com/evrimoztamur/maginet-deploy",
  "author": "evrim.zone",
  "devDependencies": {
    "electron": "^25.3.1",
    "electron-packager": "^17.1.1"
  }
}